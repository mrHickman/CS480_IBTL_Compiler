def isDigit(char):
    if char != int :
        char = ord(char)
    if char <= 57 and char >= 48 :
        return True
    else :
        return False

def isAlpha(char):
    if char != int :
        char = ord(char)
    if (char <= 90 and char >= 65) or (char <= 122 and char >= 97) :
        return True
    else :
        return False
     
