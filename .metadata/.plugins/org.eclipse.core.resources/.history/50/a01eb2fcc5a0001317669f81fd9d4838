GForthConversion = { '^'    : 'f**',
                    'sin'   : 'fsin',
                    'cos'   : 'fcos',
                    '%'     : 'mod',
                    '+'     : 'f+',
                    '*'     : 'f*',
                    '/'     : 'f/',  
                    '-'     : 'f-'  
                    }