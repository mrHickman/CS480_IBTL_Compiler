class ParserGFourthOutput:
    '''
    classdocs
    '''
    
    def __init__(self, parseTree):

        self.parseTree = parseTree
        self.currentNode = ''
        self.peakNode = self.parseTree.getNextLeftMostNode()
        self.getNextNode()
        self.generateGFourth()
    
    def getNextNode(self):
        self.currentNode =  self.peakNode
        self.peakNode = self.parseTree.getNextLeftMostNode()
    
    def generateGFourth(self):
        print self.currentNode.getToken().getValue()
        print self.peakNode.getValue()
        while self.peakNode:
            print self.currentNode.getValue()
            self.getNextNode()