from optparse import OptionParser

def main():
    SrcFilePath = ""
    parser = OptionParser()
    parser.add_option("-src", dest="SrcFilePath", help="Input source file path")
    (options, args) = parser.parse_args()
    print 'hello'
    print SrcFilePath
   
if __name__ == '__main__':
    main()


    